# Juno Configuration
# Get these from your Juno Console (console.juno.build)
NEXT_PUBLIC_SATELLITE_ID=your-satellite-id-here

# Paystack Configuration
# Get these from your Paystack Dashboard (dashboard.paystack.com)
NEXT_PUBLIC_PAYSTACK_PUBLIC_KEY=pk_test_xxxxxxxxxxxxx
PAYSTACK_SECRET_KEY=sk_test_xxxxxxxxxxxxx

# Stripe Configuration  
# Get these from your Stripe Dashboard (dashboard.stripe.com)
NEXT_PUBLIC_STRIPE_PUBLIC_KEY=pk_test_xxxxxxxxxxxxx
